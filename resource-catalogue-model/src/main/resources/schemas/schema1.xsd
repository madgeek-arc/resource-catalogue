<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0">
    
  <xs:complexType name="adapter">
        
    <xs:sequence>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="admins" nillable="true" type="user">
        <xs:annotation>
          <xs:documentation><![CDATA[Adapter user admins]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="catalogueId" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The Catalogue this Adapter is originally registered at.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="changeLog" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Changes in the latest version]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Description]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="documentation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Documentation webpage (e.g., read-the-docs page)]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="id" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Unique ID (automatically given)]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation><![CDATA[Latest update date]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="license" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Software/Code license (e.g., MIT, Apache, GPL)]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="linkedResource" type="linkedResource">
        <xs:annotation>
          <xs:documentation><![CDATA[EOSC Guideline or Service ID]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="logo" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[logo (image) – could be a URL or base64-encoded string]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="name" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Unique name]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="node" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Adapter's original Node]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="programmingLanguage" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Programming language]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="releases" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Links to the latest package release page(s) (e.g., PyPI project, Docker image, GitHub releases page)]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="repository" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Code repository webpage (e.g., a GitHub repository)]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="tagline" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Short catch-phrase]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="version" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Software version]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="user">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="email" type="xs:string"/>
            
      <xs:element minOccurs="0" name="id" type="xs:string"/>
            
      <xs:element minOccurs="0" name="name" type="xs:string"/>
            
      <xs:element minOccurs="0" name="surname" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="linkedResource">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="id" type="xs:string"/>
            
      <xs:element minOccurs="0" name="type" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="adapterBundle">
        
    <xs:complexContent>
            
      <xs:extension base="bundle">
                
        <xs:sequence>
                    
          <xs:element minOccurs="0" name="adapter" type="adapter"/>
                    
          <xs:element minOccurs="0" name="auditState" type="xs:string"/>
                    
          <xs:element minOccurs="0" name="status" type="xs:string"/>
                  
        </xs:sequence>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType abstract="true" name="bundle">
        
    <xs:sequence>
            
      <xs:element name="active" type="xs:boolean"/>
            
      <xs:element name="draft" type="xs:boolean"/>
            
      <xs:element minOccurs="0" name="id" type="xs:string"/>
            
      <xs:element minOccurs="0" name="identifiers" type="identifiers"/>
            
      <xs:element minOccurs="0" name="latestAuditInfo" type="loggingInfo"/>
            
      <xs:element minOccurs="0" name="latestOnboardingInfo" type="loggingInfo"/>
            
      <xs:element minOccurs="0" name="latestUpdateInfo" type="loggingInfo"/>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="loggingInfo" nillable="true" type="loggingInfo"/>
            
      <xs:element minOccurs="0" name="metadata" type="metadata"/>
            
      <xs:element minOccurs="0" name="migrationStatus" type="migrationStatus"/>
            
      <xs:element name="suspended" type="xs:boolean"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="identifiers">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="originalId" type="xs:string"/>
            
      <xs:element minOccurs="0" name="pid" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="loggingInfo">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="actionType" type="xs:string"/>
            
      <xs:element minOccurs="0" name="comment" type="xs:string"/>
            
      <xs:element minOccurs="0" name="date" type="xs:string"/>
            
      <xs:element minOccurs="0" name="type" type="xs:string"/>
            
      <xs:element minOccurs="0" name="userEmail" type="xs:string"/>
            
      <xs:element minOccurs="0" name="userFullName" type="xs:string"/>
            
      <xs:element minOccurs="0" name="userRole" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="metadata">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="modifiedAt" type="xs:string"/>
            
      <xs:element minOccurs="0" name="modifiedBy" type="xs:string"/>
            
      <xs:element name="published" type="xs:boolean"/>
            
      <xs:element minOccurs="0" name="registeredAt" type="xs:string"/>
            
      <xs:element minOccurs="0" name="registeredBy" type="xs:string"/>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="terms" nillable="true" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="migrationStatus">
        
    <xs:sequence>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="comments" nillable="true" type="xs:string"/>
            
      <xs:element minOccurs="0" name="migrationDate" type="xs:string"/>
            
      <xs:element minOccurs="0" name="modelVersion" type="xs:string"/>
            
      <xs:element minOccurs="0" name="modified" type="xs:string"/>
            
      <xs:element minOccurs="0" name="resolutionDate" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="adminAuthentication">
        
    <xs:sequence>
            
      <xs:element name="authenticated" type="xs:boolean"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="alternativeIdentifier">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="type" type="xs:string"/>
            
      <xs:element minOccurs="0" name="value" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="catalogue">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="abbreviation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[An abbreviation of the (Multi-Provider Regional or Thematic) Catalogue Name.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="affiliations" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogues that are members or affiliated or associated with other organisations should list those organisations here.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A high-level description of the Catalogue in fairly non-technical terms, with the vision, mission, objectives, background, experience.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="endOfLife" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[In terms of sustainability, what is the expected life of the Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="hostingLegalEntity" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Name of the organisation legally hosting (housing) the Catalogue or its coordinating centre.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="id" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A persistent identifier, a unique reference to the (Multi-Provider Regional or Thematic) Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="inclusionCriteria" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Inclusion Criteria]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element name="legalEntity" type="xs:boolean">
        <xs:annotation>
          <xs:documentation><![CDATA[A Y/N question to define whether the (Multi-Provider Regional or Thematic) Catalogue is owned by a Legal Entity or not.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="legalStatus" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Legal status of the (Multi-Provider Regional or Thematic ) Catalogue Owner. The legal status is usually noted in the registration act/statutes.
For independent legal entities (1) - legal status of the Catalogue. For embedded Catalogues (2) - legal status of the hosting legal entity.
It is also possible to select Not a legal entity.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="location" type="providerLocation">
        <xs:annotation>
          <xs:documentation><![CDATA[Physical location of the Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="logo" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to the logo/visual identity of the Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="mainContact" type="providerMainContact">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogue's main contact info.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="multimedia" nillable="true" type="multimediaPair">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to video, slideshow, photos, screenshots with details of the Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="name" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Full Name of the (Multi-Provider Regional or Thematic) Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="networks" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogues that are members of networks should list those networks here.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="node" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogue's original Node]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="participatingCountries" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Catalogues that are funded/supported by several countries should list here all supporting countries (including the Coordinating country).]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="publicContacts" nillable="true" type="providerPublicContact">
        <xs:annotation>
          <xs:documentation><![CDATA[List of the Catalogue's public contacts info.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="scientificDomains" nillable="true" type="serviceProviderDomain">
        <xs:annotation>
          <xs:documentation><![CDATA[A named group of providers that offer access to the same type of resource or capabilities.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="scope" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A high-level description of the Catalogue's scope.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="tags" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Keywords associated to the Catalogue to simplify search by relevant keywords.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element maxOccurs="unbounded" minOccurs="0" name="users" nillable="true" type="user"/>
            
      <xs:element minOccurs="0" name="validationProcess" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Validation Process]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="website" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Website with information about the (Multi-Provider Regional or Thematic) Catalogue.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="providerLocation">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="city" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[City of incorporation or Physical location of the resource or its coordinating centre in the case of distributed, virtual, and mobile resources.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="country" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Country of incorporation or Physical location of the resource or its coordinating centre in the case of distributed, virtual, and mobile resources.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="postalCode" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Postal code of incorporation or Physical location of the resource or its coordinating centre in the case of distributed, virtual, and mobile resources.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="region" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Region of incorporation or Physical location of the resource or its coordinating centre in the case of distributed, virtual, and mobile resources.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="streetNameAndNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Street and Number of incorporation or Physical location of the resource or its coordinating centre in the case of distributed, virtual, and mobile resources.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="providerMainContact">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Email of the resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="firstName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[First Name of the Provider's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="lastName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Last Name of the resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Phone of the resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="position" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Position of the resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="multimediaPair">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="multimediaName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Short description of the Multimedia content.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="multimediaURL" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to video, slideshow, photos, screenshots with details of the resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="providerPublicContact">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Email of the resource's public contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="firstName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[First Name of the resource's public contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="lastName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Last Name of the resource's public contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Phone of the resource's public contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="position" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Position of the resource's public contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="serviceProviderDomain">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="scientificDomain" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The branch of science, scientific discipline that is related to the resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
            
      <xs:element minOccurs="0" name="scientificSubdomain" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The sub-branch of science, scientific sub-discipline that is related to the resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="catalogueBundle">
        
    <xs:complexContent>
            
      <xs:extension base="bundle">
                
        <xs:sequence>
                    
          <xs:element minOccurs="0" name="auditState" type="xs:string"/>
                    
          <xs:element minOccurs="0" name="catalogue" type="catalogue"/>
                    
          <xs:element minOccurs="0" name="status" type="xs:string"/>
                    
          <xs:element maxOccurs="unbounded" minOccurs="0" name="transferContactInformation" nillable="true" type="contactInfoTransfer"/>
                  
        </xs:sequence>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType name="contactInfoTransfer">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="acceptedTransfer" type="xs:boolean"/>
            
      <xs:element minOccurs="0" name="email" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="configurationTemplate">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="catalogueId" type="xs:string"/>
            
      <xs:element minOccurs="0" name="description" type="xs:string"/>
            
      <xs:element name="formModel">
                
        <xs:complexType>
                    
          <xs:sequence>
                        
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
                            
              <xs:complexType>
                                
                <xs:sequence>
                                    
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                                    
                  <xs:element minOccurs="0" name="value" type="xs:anyType"/>
                                  
                </xs:sequence>
                              
              </xs:complexType>
                          
            </xs:element>
                      
          </xs:sequence>
                  
        </xs:complexType>
              
      </xs:element>
            
      <xs:element minOccurs="0" name="id" type="xs:string"/>
            
      <xs:element minOccurs="0" name="interoperabilityRecordId" type="xs:string"/>
            
      <xs:element minOccurs="0" name="name" type="xs:string"/>
            
      <xs:element minOccurs="0" name="node" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="configurationTemplateBundle">
        
    <xs:complexContent>
            
      <xs:extension base="bundle">
                
        <xs:sequence>
                    
          <xs:element minOccurs="0" name="configurationTemplate" type="configurationTemplate"/>
                  
        </xs:sequence>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType name="configurationTemplateInstance">
        
    <xs:sequence>
            
      <xs:element minOccurs="0" name="catalogueId" type="xs:string"/>
            
      <xs:element minOccurs="0" name="configurationTemplateId" type="xs:string"/>
            
      <xs:element minOccurs="0" name="id" type="xs:string"/>
            
      <xs:element minOccurs="0" name="node" type="xs:string"/>
            
      <xs:element name="payload">
                
        <xs:complexType>
                    
          <xs:sequence>
                        
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
                            
              <xs:complexType>
                                
                <xs:sequence>
                                    
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                                    
                  <xs:element minOccurs="0" name="value" type="xs:anyType"/>
                                  
                </xs:sequence>
                              
              </xs:complexType>
                          
            </xs:element>
                      
          </xs:sequence>
                  
        </xs:complexType>
              
      </xs:element>
            
      <xs:element minOccurs="0" name="resourceId" type="xs:string"/>
          
    </xs:sequence>
      
  </xs:complexType>
    
  <xs:complexType name="configurationTemplateInstanceBundle">
        
    <xs:complexContent>
            
      <xs:extension base="bundle">
                
        <xs:sequence>
                    
